@page
@model Web.IdP.Pages.Admin.UsersModel
@inject Microsoft.Extensions.Localization.IStringLocalizer<Web.IdP.SharedResource> Localizer
@{
    ViewData["Title"] = Localizer["Users"].Value;
    ViewData["Breadcrumb"] = Localizer["Users"].Value;
}

@inject Microsoft.AspNetCore.Antiforgery.IAntiforgery Antiforgery

<div id="admin-users-app" class="vue-app-container" data-csrf-token="@Antiforgery.GetAndStoreTokens(ViewContext.HttpContext).RequestToken"></div>
<script type="module" vite-src="~/src/admin/users/main.js"></script>

@page
@model Web.IdP.Pages.Admin.ScopesModel
@inject Microsoft.Extensions.Localization.IStringLocalizer<Web.IdP.SharedResource> Localizer
@{
    ViewData["Title"] = Localizer["Scopes"].Value;
    ViewData["Breadcrumb"] = Localizer["Scopes"].Value;
    if (ViewData["DynamicLayout"] != null)
    {
        Layout = ViewData["DynamicLayout"]?.ToString();
    }
}

<div id="app" class="vue-app-container"></div>
<script type="module" vite-src="~/src/admin/scopes/main.js"></script>
